{
  "name": "sveltekit-blog-mdx",
  "version": "0.5.1",
  "scripts": {
    "dev": "svelte-kit dev --port 3030",
    "build": "npm run generate:manifest && svelte-kit build",
    "preview": "svelte-kit preview --port 3030",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "lint:scss": "stylelint \"src/**/*.{css,scss,svelte}\"",
    "format": "prettier --write --plugin-search-dir=. .",
    "generate:manifest": "node ./generate-manifest.js",
    "generate:sitemap": "node ./generate-sitemap.js",
    "prettier:check": "prettier --check --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@fontsource/lato": "^4.5.0",
    "@fontsource/lora": "^4.5.0",
    "@fontsource/source-sans-pro": "^4.5.0",
    "@imgix/js-core": "^3.2.1",
    "@sveltejs/adapter-static": "^1.0.0-next.17",
    "@sveltejs/kit": "^1.0.0-next.159",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "husky": "^7.0.2",
    "mdsvex": "^0.9.8",
    "object-hash": "^2.2.0",
    "prettier": "~2.3.2",
    "prettier-plugin-svelte": "^2.4.0",
    "reading-time": "^1.4.0",
    "sass": "^1.38.2",
    "sharp": "^0.29.0",
    "stylelint": "^13.13.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.20.1",
    "svelte": "^3.42.4",
    "svelte-feather-icons": "^3.5.0",
    "svelte-preprocess": "^4.8.0",
    "svelte-preprocess-import-assets": "^0.1.3",
    "vanilla-lazyload": "^17.4.0",
    "vite-imagetools": "^3.6.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodneylab/sveltekit-seo"
  },
  "bugs": {
    "url": "https://github.com/rodneylab/sveltekit-seo/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pnpm run prettier:check && pnpm run lint:scss"
    }
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/adapter-netlify": "^1.0.0-next.29",
    "front-matter": "^4.0.2"
  }
}
