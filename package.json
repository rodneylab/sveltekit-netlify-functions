{
  "name": "sveltekit-blog-mdx",
  "version": "0.5.1",
  "scripts": {
    "dev": "svelte-kit dev --port 3030",
    "build": "npm run generate:manifest && svelte-kit build",
    "preview": "svelte-kit preview --port 3030",
    "check": "svelte-check --fail-on-hints",
    "check:watch": "svelte-check --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "lint:scss": "stylelint \"src/**/*.{css,scss,svelte}\"",
    "format": "prettier --write --plugin-search-dir=. .",
    "generate:manifest": "node ./generate-manifest.js",
    "generate:sitemap": "node ./generate-sitemap.js",
    "prettier:check": "prettier --check --plugin-search-dir=. .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@fontsource/lato": "^4.5.0",
    "@fontsource/lora": "^4.5.1",
    "@fontsource/source-sans-pro": "^4.5.1",
    "@imgix/js-core": "^3.4.0",
    "@rodneylab/sveltekit-components": "^0.2.8",
    "@sveltejs/adapter-netlify": "^1.0.0-next.38",
    "@sveltejs/adapter-static": "^1.0.0-next.26",
    "@sveltejs/kit": "^1.0.0-next.227",
    "dayjs": "^1.10.7",
    "dotenv": "^11.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "front-matter": "^4.0.2",
    "husky": "^7.0.4",
    "mdsvex": "^0.9.8",
    "object-hash": "^2.2.0",
    "postcss": "^8.4.5",
    "prettier": "~2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "reading-time": "1.4",
    "sass": "^1.48.0",
    "sharp": "^0.29.3",
    "stylelint": "^14.2.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-scss": "^4.1.0",
    "svelte": "^3.46.1",
    "svelte-feather-icons": "^3.5.0",
    "svelte-preprocess": "^4.10.1",
    "svelte-preprocess-import-assets": "^0.1.3",
    "vanilla-lazyload": "^17.5.0",
    "vite-imagetools": "^4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodneylab/sveltekit-netlify-functions"
  },
  "bugs": {
    "url": "https://github.com/rodneylab/sveltekit-netlify-functions/issues"
  },
  "type": "module",
  "dependencies": {
    "svelte-check": "^2.3.0"
  }
}
