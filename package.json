{
  "name": "sveltekit-blog-mdx",
  "version": "0.5.1",
  "license": "BSD-3-Clause",
  "description": "Using Netlify Functions with SvelteKit: learn how you can continue using your existing Netlify functions when you move your site to SvelteKit.",
  "scripts": {
    "dev": "vite dev",
    "build": "npm run generate:manifest && vite build",
    "preview": "vite preview",
    "check": "svelte-check --fail-on-hints",
    "check:watch": "svelte-check --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "lint:scss": "stylelint \"src/**/*.{css,scss,svelte}\"",
    "format": "prettier --write --plugin-search-dir=. .",
    "generate:manifest": "node ./generate-manifest.js",
    "generate:sitemap": "node ./generate-sitemap.js",
    "prettier:check": "prettier --check --plugin-search-dir=. .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@fontsource/lato": "^4.5.9",
    "@fontsource/lora": "^4.5.11",
    "@fontsource/source-sans-pro": "^4.5.11",
    "@iconify/svelte": "^2.2.1",
    "@rodneylab/sveltekit-components": "^0.2.16",
    "@sveltejs/adapter-netlify": "^1.0.0-next.72",
    "@sveltejs/kit": "^1.0.0-next.444",
    "@types/node": "^18.7.13",
    "@types/object-hash": "^2.2.1",
    "commitlint": "^17.1.1",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "front-matter": "^4.0.2",
    "husky": "^8.0.1",
    "leaflet": "^1.8.0",
    "mdsvex": "^0.10.6",
    "object-hash": "^3.0.0",
    "postcss": "^8.4.16",
    "postcss-html": "^1.5.0",
    "prettier": "~2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "reading-time": "~1.4.0",
    "sass": "^1.54.5",
    "sharp": "^0.30.7",
    "stylelint": "^14.11.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-scss": "^4.3.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.1",
    "svelte-preprocess": "^4.10.7",
    "svelte-preprocess-import-assets": "^0.1.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "vanilla-lazyload": "^17.8.3",
    "vite": "^3.0.9",
    "vite-imagetools": "^4.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodneylab/sveltekit-netlify-functions"
  },
  "bugs": {
    "url": "https://github.com/rodneylab/sveltekit-netlify-functions/issues"
  },
  "type": "module",
  "peerDependencies": {
    "@types/node": "^18.7.13",
    "leaflet": "^1.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "vite": "^3.0.9"
  }
}
